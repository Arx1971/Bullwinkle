# ---------------------------------------------
# Example of a simple BNF grammar for arithmetic expressions in
# Polish notation (RPN)
# ---------------------------------------------

<exp> := <add> | <sub> | <mul> | <div> | <num>;
<add> := + <exp> <exp> ;
<sub> := - <exp> <exp> ;
<mul> := ร <exp> <exp> ;
<div> := รท <exp> <exp> ;

# This last declaration defines the non-terminal symbol num as any
# sequence of symbols matching the regular expression enclosed between
# the ^ and $ characters.
<num> := ^[0-9]+;
